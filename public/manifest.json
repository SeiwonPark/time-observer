{
  "manifest_version": 3,
  "name": "time-observer",
  "version": "0.0.1",
  "description": "A notifier for web usages",
  "icons": {},
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}