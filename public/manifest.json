{
  "manifest_version": 3,
  "name": "time-observer",
  "version": "0.0.1",
  "description": "A notifier for web usages",
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "64": "assets/icon64.png",
    "128": "assets/icon128.png",
    "256": "assets/icon256.png"
  },
  "web_accessible_resources": [
    {
      "resources": [],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": []
}
